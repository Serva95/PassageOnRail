<%= form_with(model: user, local: true) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <br>

  <div class="actions">
    <%= form.submit "Login"%>
  </div>
<% end %>
